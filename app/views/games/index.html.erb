<h1>Listing games</h1>
<table>
  <tr>
    <th>Createddate</th>
    <th>Daynightfreq</th>

    <th></th>
    <th></th>
    <th></th>
  </tr>

  <% @games.each do |game| %>
      <tr>
        <td><%= game.created_at %></td>
        <td><%= game.dayNightFreq %></td>

        <td><%= link_to 'Show', game %></td>
        <td><%= link_to 'Destroy', game, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
  <% end %>
</table>

